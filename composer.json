{
    "name": "verzatiletom/vcomponent",
    "description": "A simple vue component generator using make:v-component artisan command in Laravel.",
    "type": "laravel",
    "license": "MIT",
    "keywords": ["laravel", "vue", "vue-component", "generator"],
    "authors": [
        {
            "name": "Thomas John Gulisao",
            "email": "thomasjohngulisao@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Verzatiletom\\Vcomponent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Verzatiletom\\Vcomponent\\VcomponentServiceProvider"
            ]
        }
    }
}
